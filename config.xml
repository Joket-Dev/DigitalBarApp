<?xml version='1.0' encoding='utf-8'?>
<widget id="com.digitalbar" version="8.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Digitalbar</name>
    <description>
        ibeacon testing.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <resource-file src="www/onesignal_default_sound.wav" target="res/raw/onesignal_default_sound.wav" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
    <icon src="res/icon.png" />
    <plugin name="cordova-plugin-x-socialsharing" spec="^5.4.1" />
    <feature name="SocialSharing">
        <param name="ios-package" value="SocialSharing" />
    </feature>
    <feature name="SocialSharing">
        <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
    </feature>
    <feature name="SocialSharing">
        <param name="wp-package" value="SocialSharing" />
    </feature>
    <plugin name="cordova-android-support-gradle-release" spec="^1.4.4">
        <variable name="ANDROID_SUPPORT_VERSION" value="27.+" />
    </plugin>
    <plugin name="onesignal-cordova-plugin" spec="^2.4.5" />
    <plugin name="com.unarin.cordova.beacon" spec="https://github.com/petermetz/cordova-plugin-ibeacon.git" />
    <plugin name="cordova-plugin-badge" spec="^0.8.8" />
    <plugin name="cordova-plugin-ezar-snapshot" spec="^1.0.0" />
    <plugin name="cordova-plugin-ezar-facedetector" spec="^1.0.0" />
    <plugin name="cordova-plugin-ezar-videooverlay" spec="^1.0.0" />
    <plugin name="cordova-plugin-face-recognition" spec="^1.0.0" />
    <plugin name="com.razorpay.cordova" spec="^1.4.3" />
    <engine name="android" spec="^7.1.4" />
</widget>
